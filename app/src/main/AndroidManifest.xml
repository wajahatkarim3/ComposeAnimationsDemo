        android:supportsRtl="true"
        android:theme="@style/Theme.DroidconEMEA2020">
        <activity
            android:name=".DroidconActivity"
            android:label="@string/title_activity_droidcon"
            android:theme="@style/Theme.DroidconEMEA2020.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>